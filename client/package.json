{
  "name": "club-mutant",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.6.4",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@5.6.1",
    "@reduxjs/toolkit": "^1.6.1",
    "antd": "^4.16.13",
    "axios": "^1.6.5",
    "@colyseus/sdk": "^0.17.22",
    "peerjs": "^1.3.2",
    "phaser": "^3.90.0",
    "phaser3-rex-plugins": "^1.80.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.10.0",
    "react-redux": "^7.2.5",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.42.1",
    "styled-components": "^5.3.1",
    "swiper": "^8.1.4",
    "tslib": "^2.6.2",
    "@club-mutant/types": "workspace:*",
    "typescript": "^5.7.0",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "tsc && vite build",
    "build:electron": "tsc electron/main.ts electron/preload.ts --outDir electron --module commonjs --esModuleInterop --target es2020 --declaration",
    "preview": "vite preview",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "postinstall": "if [ -z \"$NETLIFY\" ]; then electron-builder install-app-deps; fi"
  },
  "build": {
    "appId": "com.clubmutant.app",
    "productName": "Club Mutant",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*.js",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.games"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.7.3",
    "vite": "^3.0.9"
  }
}
